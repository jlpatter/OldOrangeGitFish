    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^15.3.1",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "bootstrap": "^5.1.3",
