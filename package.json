{
  "name": "orangegitfish",
  "version": "0.1.5",
  "description": "A git gui application",
  "main": "backend/index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "Joshua Patterson <joshua.l.patterson7@gmail.com>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "electron": "^15.3.1",
    "electron-builder": "^22.14.5",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "jquery": "^3.6.0",
    "keytar": "^7.7.0",
    "nodegit": "^0.28.0-alpha.10",
    "popper.js": "^1.16.1"
  },
  "build": {
    "appId": "com.jlpatter.orangegitfish",
    "compression": "normal",
    "asar": true,
    "extends": null,
    "files": [
      "./backend/*",
      "./frontend/**/*",
      "./lib/bootstrap/5.1.3/**/*",
      "node_modules/**/*",
      "./fish.png"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "tar.xz"
      ],
      "icon": "./fish.png",
      "category": "Development"
    },
    "mac": {
      "icon": "./fish.icns"
    },
    "nsis": {
      "oneClick": "false"
    },
    "win": {
      "target": "nsis",
      "icon": "./fish.ico"
    }
  }
}
